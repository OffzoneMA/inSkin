
{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/swagger-ui/(.*)",
        "dest": "./node_modules/swagger-ui-dist/$1",
        "headers": {
        "Access-Control-Allow-Origin": "*"  
        }
      },
      {
        "src": "/api-docs",
        "dest": "/api-docs"
      },
      {
        "src": "/(.*)",
        "dest": "./"
      }
    ]
  }
